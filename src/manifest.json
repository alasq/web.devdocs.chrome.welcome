{
  "name": "前端新页签",
  "version": "0.0.1",
  "manifest_version": 2,
  "author": "Chen shiqiao <chen4q@163.com>",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {},
  "options_page": "option.html"
}
